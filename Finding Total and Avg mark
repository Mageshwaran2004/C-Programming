#include <stdio.h>
int main()
{
    int m1,m2,m3,m4,m5,total;
    //5 different subjects
    float avg;
    printf("\nEnter your 5 Different subject marks: ");
    scanf("%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5);
    total=m1+m2+m3+m4+m5;
    avg=total/5;
    printf("\nYour total mark is: %d ",total);
    printf("\nYour average mark is: %f ",avg);
    return 0;
}
