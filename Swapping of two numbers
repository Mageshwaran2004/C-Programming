#include <stdio.h>
int main()
{

    int b,c,d;
    printf("\nEnter 2 Numbers: ");
    scanf("%d%d",&c,&d);
    printf("\n C:%d \n D:%d ",c,d);
    b=c;
    c=d;
    d=b;
    printf("\n Interchanged values of C:%d \n Interchanged values of D:%d",c,d);
    return 0;

}
we can interchange the values of two numbers without using the third variable;
consider the same c and d as input; //c=5 d=10
c=c+d; //15
d=c-d; //5
c=c-d; //10
