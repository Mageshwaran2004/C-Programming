#include<stdio.h>
int main()
{
    int m1,m2,m3,m4,m5,tot,avg,mark;
    printf("\nEnter the 5 subject marks: ");
    scanf("%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5);
    tot=m1+m2+m3+m4+m5;
    avg=tot/5;
    printf("\nTotal Marks is:%d",tot);
    printf("\nAverage Marks is:%d",avg);
    if(m1>=35&&m2>=35&&m3>=35&&m4>=35&&m5>=35)
    {
        printf("\nResult:PASS");
        if(avg>=90&&avg<=100)
        {
            printf("\nGrade: A Grade");
        }
        else if (avg>=80&&avg<=89)
        {
            printf("\nGrade: B Grade");
        }
        else if (avg>=70&&avg<=79)
        {
            printf("\nGrade: C Grade");
        }
        else if (avg<70)
        {
            printf("\nGrade: D Grade");
        }
        else
        {
            printf("\nResult:FAIL");
            printf("\nResult:No Grade");
        }
    }
    return 0;
}
